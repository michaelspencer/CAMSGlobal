<% content_for :title do %>CAMSGroup<% end %>

<!-- search form -->
<%= render :partial => "shared/searchform" %> 

<div id="page_title">
	<p>Import Website Pages</p>
</div>

<div id="content">
	<div class="text">

		<% form_tag(:controller => "posts", :action => "import_submit", :method => "post", :html => { :multipart => true }) do %>  
			<p><%= label_tag(:file, "Select a CSV file:") %></p>
			<p><%= file_field_tag(:file) %></p>
			<p><%= submit_tag("Import") %></p>
		<% end %> 

	</div> <!--ends text -->
</div> <!-- ends content -->
